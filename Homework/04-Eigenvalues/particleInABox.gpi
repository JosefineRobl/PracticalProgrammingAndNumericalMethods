set terminal png;
set output "particleInABox.png"

set key bottom left;
set ticks out;
set pointsize 10;
set width 8;

set title "\textbf{Quantum particle in a box}";
set xlabel "$x$";
set ylabel "$\psi$";
set yrange [-1.75:1.5];

plot \
"particleInABoxEigenfunctions.txt" using 1:3 with line linetype 1 color red title "Own implementation",\
"" using 1:2 with dots color black title "Exact";
